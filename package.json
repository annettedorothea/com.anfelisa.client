{
  "name": "anfelisa",
  "version": "0.9.0",
  "description": "anfelisa client",
  "repository": "https://github.com/annettedorothea/com.anfelisa.client",
  "scripts": {
    "uglify": "uglifyjs lib/anfelisa_0.9.0.js --compress --mangle --output lib/anfelisa_0.9.0.min.js",
    "build": "browserify --entry es6/src/app/App.js --transform [ babelify --presets [ @babel/preset-env @babel/preset-react ] ] --outfile lib/anfelisa_0.9.0.js --debug --standalone Anfelisa",
    "start": "watchify --entry es6/src/app/App.js --transform [ babelify --presets [ @babel/preset-env @babel/preset-react ] ] --outfile lib/anfelisa_0.9.0.js --debug --standalone Anfelisa & live-server --watch=lib/anfelisa_0.9.0.js --host=127.0.0.1 --port=8888 --proxy='/api/':'http://localhost:8080/api/' --proxy='/replay/':'http://localhost:8082/replay/' --browser='Google Chrome'",
    "test": "ava"
  },
  "author": "Annette Pohl",
  "license": "GPL-3.0",
  "ava": {
    "require": [
      "esm"
    ],
    "babel": true
  },
  "dependencies": {
    "@jest/globals": "^26.1.0",
    "crypto-js": "^4.0.0",
    "json-stable-stringify": "^1.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@ava/babel": "^1.0.1",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "ava": "^3.9.0",
    "babelify": "^10.0.0",
    "browserify": "^16.5.1",
    "esm": "^3.2.25",
    "live-server": "^1.2.1",
    "uglify-js": "^3.9.0",
    "watchify": "^3.11.1"
  }
}
